<%= form_for(@cost_oper_machine_cont,html: {class: 'form-horizontal',"data-fv-framework": "bootstrap", id: "form_cost_oper_machine_conts", remote: true,"data-type": "json",'data-model' => 'cost_oper_machine_cont'}) do |f| %>      <div class="col-sm-6 col-lg-12 col-md-12 col-xs-12">         <div class="row">            <div class="col-lg-5">               <div class="form-group">                  <%= f.label :program_production_id,"Parcela:" ,class: "control-label col-sm-6 col-lg-5 col-xs-12"%>                  <div class="col-lg-6">                     <input value="<%= @farm_name %>" class="form-control" readonly/>                  </div>               </div>            </div>            <div class="col-lg-4">               <div class="form-group">                  <%= f.label :program_production_id,"Área :" ,class: "control-label col-sm-6 col-lg-3 col-xs-12"%>                  <div class="col-lg-6 col-md-2">                     <div class="input-group">                        <input id="farm_area" value="<%=  @farm_area %>" class="form-control" readonly/>                        <span class="input-group-addon" id="basic-addon2">Ha</span>                     </div>                  </div>               </div>            </div>         </div>         <div class="row">            <div class="col-lg-5">               <div class="form-group">                  <%= f.label :program_production_id,"Tipo de servicio:" ,class: "control-label col-lg-5"%>                  <div class="col-sm-6 col-md-6 col-xs-12 col-lg-7">                    <%= select("cost_oper_cont_detail","type_of_service_id", options_for_select(@type_services), {}, {required: 'true', class: "form-control", include_blank: ''})%>                     </div>               </div>            </div>            <div class="col-lg-3">               <div class="form-group">                  <label class="control-label col-lg-4" >U.m :</label>                  <div class="col-sm-6 col-md-6 col-xs-12 col-lg-6">                    <%= select("cost_oper_cont_detail","unit_measure", options_for_select(@unit_measure), {}, {required: 'true', class: "form-control", include_blank: ''})%>                     </div>               </div>            </div>            <div class="col-lg-4">               <div class="form-group">                  <%= f.label :description,"Tiempo aprox :" ,class: "control-label col-lg-6"%>                  <div class="col-sm-6 col-md-6 col-xs-12 col-lg-6">                     <div class="input-group">                        <input id="time_aprox" name="time_aprox" value="" class="form-control" disabled/>                        <span class="input-group-addon" id="basic-addon2">hs</span>                                     </div>                  </div>               </div>            </div>         </div>         <div class="row">            <div class="col-lg-5">               <div class="form-group">                  <%= f.label :description,"Cantidad:" ,class: "control-label col-lg-5"%>                  <div class="col-sm-6 col-md-6 col-xs-12 col-lg-6">                     <input id="cost_oper_cont_detail_amount" name="cost_oper_cont_detail[amount]" value="1" class="form-control only_numbers"                     data-fv-message="The username is not valid"                required data-fv-notempty-message="The username is required and cannot be empty"                pattern="^[a-zA-Z0-9]+$" data-fv-regexp-message="The username can only consist of alphabetical, number" />                  </div>               </div>            </div>            <div class="col-xs-12 col-lg-2 text-center">               <a href="#" class="btn btn-default btn-sm" id="btn_add_cost_out">Agregar</a>            </div>              </div>         <table                id="tb_co_mach_cont"                data-width="100%"                 data-search="false"                               class="table table-hover table-striped table-condensed display"                data-toggle="table"               data-show-toggle="false"                               data-show-refresh="false"                data-pagination="true"                onlyInfoPagination="false"                data-icons="icons" data-sort-name="code" data-sort-order="desc"                data-card-view="false"               data-checkbox-header="false"         >            <thead>               <tr>                  <th data-field="id" data-checkbox="true"></th>                  <th data-field="code">#</th>                  <th data-field="type_of_service">Tipo de servício</th>                  <th data-field="type_of_service_id" class="hide"></th>                  <th data-field="price">Precio del servicio</th>                  <th data-field="u_m">U.m </th>                  <th data-field="amount">Cantidad</th>                  <th data-field="amount_id" class="hide"></th>                  <th data-halign='right' data-id="subtotal" data-align='right' data-field="subtotal" >Sub-total</th>                  <th data-field="subtotal_id" class="hide"></th>                  <th data-width="50" data-halign='right' data-align='right' data-field="Action" data-events="remove">Acción</th>               </tr>            </thead>            <tbody>               <% if (controller.action_name == "edit") %>                     <% @cost_oper_machine_cont_details.each do |details| %>                        <tr>                           <td><%= details.id %></td>                           <td><%= details.id %></td>                           <td><%= details.type_of_service.name %></td>                           <td></td>                           <td><%= number_with_precision( details.type_of_service.price, :precision => 0, :delimiter => '.' ) %></td>                           <td></td>                           <td><%= number_with_precision( details.amount, :precision => 2, :delimiter => '.' ) %></td>                           <td></td>                           <td><%=number_with_precision( details.subtotal, :precision => 0, :delimiter => '.' ) %></td>                           <td></td>                           <td>                              <a class="change_status btn btn-danger btn-sm" href="" title="Cambiar status" >                                 <i class="fa fa-1x fa-minus"></i>                              </a>                           </td>                        </tr>                     <% end %>                      <div class="hide">                        <%= f.text_field :total, :value => @total ,:id => "total" %>                     </div>                     <% else %>                     <div class="hide">                        <%= f.text_field :total, :value => 0 ,:id => "total" %>                     </div>                  <% end %>                     </tbody>                         <tfoot>               <tr>                  <td class="h6 font-weight-bold" colspan="2"><strong>Total :<strong></td>                  <td colspan="8" class="font-weight-bold text-right">                     <strong><span class="total"><%=number_with_precision( @total, :precision => 0, :delimiter => '.' ) %></span><strong>                  </td>               </tr>            </tfoot>         </table>         <input type="hidden" name="cost_oper_machine_cont[program_production_id]" id="program_production_id" value="<%= @program_production_id%>" />        </div><%end%><input type="hidden" id="controller" value="<%= controller_name%>">