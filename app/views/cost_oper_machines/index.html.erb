<p id="notice"><%= notice %></p>

<h1>Cost Oper Machines</h1>

<table>
  <thead>
    <tr>
      <th>Farming plot</th>
      <th>Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cost_oper_machines.each do |cost_oper_machine| %>
      <tr>
        <td><%= cost_oper_machine.farming_plot %></td>
        <td><%= cost_oper_machine.total %></td>
        <td><%= link_to 'Show', cost_oper_machine %></td>
        <td><%= link_to 'Edit', edit_cost_oper_machine_path(cost_oper_machine) %></td>
        <td><%= link_to 'Destroy', cost_oper_machine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cost Oper Machine', new_cost_oper_machine_path %>
