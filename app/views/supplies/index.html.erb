<div class="panel-body">	<div class="table-responsive">		<table 		data-width="100%" 		data-height="700" 		data-flat="true"		data-response-handler="flatJSON"		data-search="true"                		class="table table-hover table-striped table-condensed display" 		data-toggle="table"		data-show-toggle="true"                		data-show-refresh="false" 		data-pagination="true" 		onlyInfoPagination="false" 		data-icons="icons" data-sort-name="program_production_id" data-sort-order="desc" 		data-card-view="false"		>		<thead>			<tr>				<th style="width:3%" data-field="id">#</th> 				<th style="width:3%" data-sortable="true" data-field="program_production_id">Parcela</th> 				<th style="width:3%" data-sortable="true" data-halign="right" data-field="total">Total</th> 				<th style="width:3%" data-halign="right" data-align="right" data-formatter="actionFormatter" data-events="actionEvents">Acci√≥n</th>			</tr>		</thead>		<tbody>			<% @supplies.each do |supply| %>			<tr>				<td><%= supply.id %></td>				<td><%= supply.program_production.estimate_sale.farming_plot.name %></td>				<td><%= number_with_precision( supply.total, :precision => 0, :delimiter => '.' )  %></td>			</tr>			<% end %>		</tbody>	</table></div></div><input type="hidden" id="controller" value="<%= controller_name%>">