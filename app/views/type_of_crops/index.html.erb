<p id="notice"><%= notice %></p>

<h1>Type Of Crops</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Variety</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_of_crops.each do |type_of_crop| %>
      <tr>
        <td><%= type_of_crop.code %></td>
        <td><%= type_of_crop.name %></td>
        <td><%= type_of_crop.variety %></td>
        <td><%= link_to 'Show', type_of_crop %></td>
        <td><%= link_to 'Edit', edit_type_of_crop_path(type_of_crop) %></td>
        <td><%= link_to 'Destroy', type_of_crop, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Of Crop', new_type_of_crop_path %>
